# 钉钉机器人配置
DINGTALK_ACCESS_TOKEN=your_dingtalk_access_token_here
DINGTALK_SECRET=your_dingtalk_secret_here
DINGTALK_AT_MOBILES=13800138000,13800138001

# Binance API配置（可选，用于私有API访问）
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here

# 市场监控配置
KLINE_INTERVAL=1h          # K线间隔: 1m, 3m, 5m, 15m, 30m, 1h, 2h, 4h, 6h, 8h, 12h, 1d, 3d, 1w, 1M
KLINE_LIMIT=56            # K线数量限制，默认56条
SYMBOL_LIMIT=50           # 监控的交易对数量，默认50个

# WebSocket重连配置
WEBSOCKET_MAX_RETRIES=5   # WebSocket最大重试次数，默认5次
WEBSOCKET_RETRY_DELAY=10  # WebSocket初始重试延迟（秒），默认10秒
WEBSOCKET_MAX_RETRY_DELAY=300  # WebSocket最大重试延迟（秒），默认300秒（5分钟）
WEBSOCKET_CONNECTION_CHECK_INTERVAL=30  # WebSocket连接检查间隔（秒），默认30秒

# 自定义交易对配置（可选）
# 如果设置了CUSTOM_SYMBOLS，将使用自定义交易对列表
# 如果没有设置，将使用常用交易对列表
# 支持JSON数组格式，例如: ["BTCUSDT","ETHUSDT","BNBUSDT"]
# 也支持逗号分隔格式（向后兼容），例如: BTCUSDT,ETHUSDT,BNBUSDT
CUSTOM_SYMBOLS=["BTCUSDT","ETHUSDT","BNBUSDT","ADAUSDT","SOLUSDT","HUSDT","FUNUSDT","DOGEUSDT","SUIUSDT","WIFUSDT"] 